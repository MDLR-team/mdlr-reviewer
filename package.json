{
  "name": "mdlr-reviewer",
  "version": "1.0.4",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "keywords": [
    "typescript",
    "react",
    "mui",
    "emotion",
    "styled-components",
    "markdown",
    "split",
    "react-split",
    "ai",
    "nextjs",
    "openai",
    "insights",
    "supabase"
  ],
  "devDependencies": {
    "eslint": "^9.15.0",
    "prettier": "^3.3.3",
    "typescript": "^5.7.2"
  },
  "engines": {
    "pnpm": "9.x"
  },
  "scripts": {
    "build": "pnpm -r --workspace-concurrency=1 run build",
    "build:p": "pnpm -r --filter @mdlr-reviewer/project --filter @mdlr-reviewer/ui-kit run build",
    "dev": "pnpm -r --workspace-concurrency=1 run dev",
    "dev:platform": "pnpm -r --filter mdlr-platform run dev",
    "lint": "pnpm -r --workspace-concurrency=1 run lint",
    "publish:p": "pnpm -r --filter @mdlr-reviewer/project --filter @mdlr-reviewer/ui-kit publish --access public"
  }
}
